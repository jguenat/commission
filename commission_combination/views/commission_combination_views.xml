<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="commission_combination_tree" model="ir.ui.view">
        <field name="name">commissions combination tree</field>
        <field name="model">commission.combination</field>
        <field name="arch" type="xml">
            <tree editable="top">
                <field name="name" />
                <field name="agent_id" />
                <field name="commission_id" />
            </tree>
        </field>
    </record>
    <record id="action_commission_combination" model="ir.actions.act_window">
        <field name="name">Commission combination</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">commission.combination</field>
        <field name="binding_view_types">tree</field>
    </record>
    <menuitem
        name="Commission combination"
        id="menu_commission_combination"
        parent="commission.menu_commission_management"
        action="action_commission_combination"
    />
</odoo>